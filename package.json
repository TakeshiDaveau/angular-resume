{
  "name": "my-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot",
    "build:staging": "ng build --aot --build-optimizer",
    "build:production": "ng build --aot --prod --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "export NODE_OPTIONS=--max_old_space_size=4096 && npm run lint -- --fix=true",
    "stylelint": "stylelint 'src/**/*.scss'",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run tda-resume:server:production --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.7",
    "@angular/common": "^9.0.0-rc.7",
    "@angular/compiler": "^9.0.0-rc.7",
    "@angular/core": "^9.0.0-rc.7",
    "@angular/forms": "^9.0.0-rc.7",
    "@angular/localize": "^9.0.0-rc.0",
    "@angular/platform-browser": "^9.0.0-rc.7",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.7",
    "@angular/platform-server": "^9.0.0-rc.7",
    "@angular/router": "^9.0.0-rc.7",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "core-js": "^3.6.1",
    "express": "^4.15.2",
    "protractor": "^5.4.2",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.7",
    "@angular/cli": "^9.0.0-rc.7",
    "@angular/compiler-cli": "^9.0.0-rc.7",
    "@angular/language-service": "^9.0.0-rc.7",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.1.1",
    "codelyzer": "^5.2.1",
    "git-describe": "^4.0.4",
    "husky": "^3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "prettier": "^1.19.1",
    "protractor-image-comparison": "^3.9.0",
    "sass-mq": "^5.0.1",
    "stylelint": "^12.0.1",
    "stylelint-config-recommended-scss": "^4.1.0",
    "stylelint-scss": "^3.13.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~8.5.4",
    "tslint": "~5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "~3.6.4",
    "webpack-cli": "^3.1.0"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 10-11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix && npm run stylelint"
    }
  }
}
